{
  "name": "echo",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: serverless-hipchat-connect",
  "customName": false,
  "customRole": false,
  "handler": "functions/echo/handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "echo",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": "$${apiGatewayTemplate}"
      },
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "204",
          "responseParameters": {},
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "INSTALLATION_TABLE": "${installationTableName}",
    "ACCESS_TOKEN_TABLE": "${accessTokenTableName}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
